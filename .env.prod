# RAGFlow Production Environment (Unraid)
# PostgreSQL - Separate Docker instance on postgres network
POSTGRES_HOST=postgresql16
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DBNAME=ragflow_db

# Elasticsearch - Local Docker service
ES_HOST=elasticsearch
ES_PORT=9200
ES_USER=elastic
ELASTIC_PASSWORD=infini_rag_flow

# Redis - Local Docker service
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=

# MinIO - Local Docker service
MINIO_HOST=minio
MINIO_PORT=9000
MINIO_USER=rag_flow
MINIO_PASSWORD=infini_rag_flow
MINIO_BUCKET=ragflow

# Python/Application settings
PYTHONPATH=/ragflow
HF_ENDPOINT=https://huggingface.co

# API Server
API_PORT=9380
API_WORKERS=4

# Web UI
WEB_PORT=8501

# Logging
LOG_LEVEL=INFO
